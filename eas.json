{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "cache": {
        "disabled": true
      },
      "node": "20.19.1",
      "ios": {
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.4-xcode-16.3",
        "resourceClass": "m-medium"
      },
      "hooks": {
        "preInstall": "./eas-hooks/eas-build-pre-install.sh"
      }
    },
    "production-stable": {
      "extends": "production",
      "ios": {
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.4-xcode-16.3",
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
