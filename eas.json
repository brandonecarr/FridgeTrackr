{
  "cli": {
    "version": ">= 13.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "cache": {
        "disabled": true
      },
      "ios": {
        "buildConfiguration": "Release",
        "config": {
          "suppress_xcode_output": false
        },
        "env": {
          "RCT_NEW_ARCH_ENABLED": "1"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
